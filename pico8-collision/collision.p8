pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function _init()
  button = ""

  player = {}
  player.sp = 1
  player.x = 63
  player.y = 63
  player.forward = false
end

function _update()
  if btn(➡️) then
    player.x += 1
    player.forward = false
    button = "right"
  end

  if btn(⬅️) then
    player.x -= 1
    player.forward = true
    button = "left"
  end

  if btn(⬇️) then
    player.y += 1
    button = "down"
  end

  if btn(⬆️) then
    player.y -= 1
    button = "up"
  end
end

function _draw()
  print(button)
  cls()
  map()
  spr(player.sp, player.x, player.y, 1, 1, player.forward)
end

